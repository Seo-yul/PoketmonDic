CREATE TABLE poketmon (
	No varchar2(60) primary key,
	kor_name varchar2(60) not null,
	jap_name varchar2(60) not null,
	height varchar2(60) not null,
	weight varchar2(60) not null,
	c_rate number not null,
	g_rate varchar2(60) not null,
	type varchar2(60) not null,
	sort varchar2(60) not null,
	character varchar2(60) not null,
	h_character varchar2(60) not null,
	e_point varchar2(60) not null
);

create table manager (
    no number primary key,
    name varchar2(30) not null,
    pwd varchar2(30) not null
);

insert into manager (no, name, pwd) values (1, '윤서율', 'poketmon1234');

